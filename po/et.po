"          gnokii --getdisplaystatus\n"
"          gnokii --displayoutput\n"
"          gnokii --getprofile [start_number [end_number]] [-r|--raw]\n"
"          gnokii --setprofile\n"
"          gnokii --getactiveprofile\n"
