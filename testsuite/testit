#!/bin/bash
(
	cd ..
	make
	cd common
	make gnokii-debug || ( echo "*** Compile FAILED"; sleep 10 )
)
export HOME=`pwd`
GNOKII=../common/gnokii-debug
for A in test.?.* test.1[023456].*; do
    echo -n "Doing $A... "
    B=`echo $A | sed s/test/out/`
    source $A 2>&1 | grep -v ^GNOKII | grep -v '^Doing operation' > my$B
    if cmp my$B $B > /dev/null 2>&1; then
	echo ok
    else
	echo '*** FAILED'
	diff -u $B my$B
    fi
done